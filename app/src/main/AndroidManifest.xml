        </activity>
        <activity android:name=".BookmarkActivity"
            android:label="@string/app_name"
            android:exported="true">
        </activity>
        <meta-data
            android:name="com.naver.maps.map.CLIENT_ID"
            android:value="au2fkea00m" />
    </application>

</manifest>